{
    "name": "dustin/filesystem-local",
    "type": "library",
    "description": "A local filesystem factory for dustin/filesystem",
    "keywords": [
        "filesystem",
        "local",
        "factory"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Dustin Simon"
    }],
    "autoload": {
        "psr-4": { "Dustin\\Filesystem\\Local\\": "src/" },
        "exclude-from-classmap": ["src/Test/"]
    },
    "require": {
        "php": "^8.1",
        "league/flysystem": "~3.10",
        "league/flysystem-local": "~3.15",
        "dustin/filesystem": "dev-feature/registry"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
